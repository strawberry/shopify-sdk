{
    "name": "strawberry/shopify",
    "description": "An (opinionated) Shopify SDK for PHP",
    "require": {
        "illuminate/support": "~5.8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.0",
        "vlucas/phpdotenv": "^3.4@dev",
        "codedungeon/phpunit-result-printer": "dev-master"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Kieran Marshall",
            "email": "kieran.marshall@strawberry.co.uk"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Strawberry\\Shopify\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Strawberry\\Shopify\\Tests\\": "tests"
        }
    }
}
